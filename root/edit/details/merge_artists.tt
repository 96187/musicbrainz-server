<table class="details">
  <tr>
    <th>[%- ln('Old artist:', 'Old artists:', edit.display_data.old.size) %]</th>
    <td>
      <ul>
        [% FOR artist=edit.display_data.old %]
        <li>[% link_entity(artist) %]</li>
        [% END %]
      </ul>
    </td>
  </tr>

  <tr>
    <th>[%- l('New artist:') %]</th>
    <td>[% link_entity(edit.display_data.new) %]</td>
  </tr>
</table>
