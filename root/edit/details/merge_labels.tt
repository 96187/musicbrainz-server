<table class="details">
  <tr>
    <th>[%- ln('Old label:', 'Old labels:', edit.display_data.old.size) %]</th>
    <td>
      <ul>
        [% FOR label=edit.display_data.old %]
        <li>[% link_entity(label) %]</li>
        [% END %]
      </ul>
    </td>
  </tr>

  <tr>
    <th>[%- l('New label:') %]</th>
    <td>[% link_entity(edit.display_data.new) %]</td>
  </tr>
</table>
