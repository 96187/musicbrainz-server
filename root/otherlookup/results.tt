[% WRAPPER "layout.tt" title=l("Lookup Results") full_width=1 %]
    <div class="content">
        <h1>[% l("Lookup Results") %]</h1>
        [% IF error %]
          <p>[% error %]</p>
        [% ELSIF results %]
          [%- INCLUDE 'components/results-generic.tt' -%]
        [% ELSE %]
          <p>[% l('No results found') %]</p>
        [% END %]
        <h1>[% l("Other lookups") %]</h1>
        [%- INCLUDE 'otherlookup/form.tt' -%]
    </div>
[% END %]
